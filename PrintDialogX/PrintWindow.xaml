<Window x:Class="PrintDialogX.Internal.PrintWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Print"
        FontSize="12"
        MinWidth="750"
        MinHeight="450"
        Background="#FFF0F0F0">

    <Frame x:Name="mainFrame" NavigationUIVisibility="Hidden">
        <Frame.Content>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Vertical">
                <Image x:Name="iconImage" MaxWidth="160" MaxHeight="160" Stretch="UniformToFill" Source="Resources/LoadingImage.png"></Image>
                <TextBlock Text="Generating Document . . ." HorizontalAlignment="Center" FontSize="16" Margin="0,20,0,0"/>
            </StackPanel>
        </Frame.Content>
    </Frame>
</Window>

